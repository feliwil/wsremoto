	public static void main(String[] args) {
		Carro objeto = new Carro();
		objeto.preencherBasico(Short.parseShort(JOptionPane.showInputDialog("Digite o numero do carro: ")), JOptionPane.showInputDialog("Digite o nome da escuderia: "),Float.parseFloat(JOptionPane.showInputDialog("Digite o valor do carro: "_)));
		System.out.println(objeto.exibirTudo());
	
		 


	

		
		
		
		
	}
}
