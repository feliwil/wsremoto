package repeticao;

import javax.swing.JOptionPane;

public class DesafioWhile {

	public static void main(String[] args) {
		int ano = Integer.parseInt(JOptionPane.showInputDialog("Digite o ano entre 1900 e 2002:" ));
		while (ano<1900 || ano > 2002) {
			ano = Integer.parseInt(JOptionPane.showInputDialog("Digite o ano entre 1900 e 2002 corretamente:" ));
		}
		int mes = Integer.parseInt(JOptionPane.showInputDialog("Digite o mês, entre 1 e 12" ));
		while (mes<1 || mes > 12) {
			mes = Integer.parseInt(JOptionPane.showInputDialog("Digite o mês, entre 1 e 12, corretamente" ));
		}
				
			

	}

}
